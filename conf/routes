
GET     /                           controllers.HomeController.index

GET     /ip                         controllers.WsController.getIp

+ nocsrf
POST    /upload                     controllers.HomeController.upload


GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


GET     /api/todos                  controllers.api.TodoRestController.getAll
GET     /api/todos/:id              controllers.api.TodoRestController.getById(id: Long)
POST    /api/todos/add              controllers.api.TodoRestController.add
PUT     /api/todos/update/:id       controllers.api.TodoRestController.update(id: Long)
DELETE /api/todos/delete/:id        controllers.api.TodoRestController.delete(id: Long)


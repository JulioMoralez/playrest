GET     /api/users/:id              controllers.api.UserRestController.getById(id: Long)
+ nocsrf
POST    /api/users/add              controllers.api.UserRestController.add
+ nocsrf
PUT     /api/users/update/:id       controllers.api.UserRestController.update(id: Long)
+ nocsrf
DELETE  /api/users/delete/:id       controllers.api.UserRestController.delete(id: Long)




POST     /users                      controllers.UserController.showUsersPost
POST    /deleteUser                  controllers.UserController.deleteUser
POST    /editUser                    controllers.UserController.editUser
